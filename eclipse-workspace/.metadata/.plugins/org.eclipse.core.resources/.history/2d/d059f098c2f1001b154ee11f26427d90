package ocatest;


public class Test {
	
	 public static void main(String [] args) {
		 
		           int[] x = {1,2,3};
				  for (int i = 0; i < x.length; i++) {
					
				     int y = x[i];
				     System.out.println("Y  "+y);
				      while ( y > 0 ) { 
				          y = y / 2 ;
				      } 
				    
				   } 
				
		  }
}
