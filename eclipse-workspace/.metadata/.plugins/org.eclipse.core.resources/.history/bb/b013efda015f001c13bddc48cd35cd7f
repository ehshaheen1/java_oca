package test1;

public class NullArrayRef {

   public static int[ ] getArray() {  return null;  }
   public static void main(String[] args){
      int index = 1;
      try{
    	 // System.out.println([2]);
         getArray()[index]++;
      }
      catch (Exception e){  }  //empty catch
      System.out.println("index = " + index);
   }
}

