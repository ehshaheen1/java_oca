package a;

public class Test1 {

    private int j;
    void showJ(){
        while(j<=5){
            for( j=1; j <= 5;){
                System.out.print(j+" ");
                j++;
            }
            System.out.println();
            j++;
        }
    }
    public static void main(String[] args) {
        new Test1().showJ();
       
    }
}




