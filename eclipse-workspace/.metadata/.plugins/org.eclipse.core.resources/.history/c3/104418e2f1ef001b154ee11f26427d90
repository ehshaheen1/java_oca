package ocatest;

public class Test3 {
	
	public static void main(String[] args) {
		
		int[][] x = {
				{1,2,3,},
				{4,5,6,},
				{7,8,9},
				{0}
	            	};
		System.out.println(x.length);
		for (int i = 0; i < x.length; i++) {
			for (int j = 0; j < x[i].length; j++) {
				System.out.print(x[i][j]+ " ");
			}
			System.out.println();
		}

	}

}
