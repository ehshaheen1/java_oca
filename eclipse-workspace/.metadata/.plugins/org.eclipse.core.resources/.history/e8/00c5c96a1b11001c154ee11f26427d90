package a;

import b.Test2;

public class Test1 extends Test2{

    public int h = 44;
    public int getH() {
        System.out.println("Beta " + h);
        return h;
    }
    public static void main(String[] args) {
        Test2 b = new Test1();
        System.out.println(b.h + " " + b.getH());
        Test1 bb = (Test1) b;
        System.out.println(bb.h + " " + bb.getH());
    }
}



