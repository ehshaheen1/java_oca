package ocatest;

import java.io.IOException;

class Parent {
    Parent() throws IOException {
        System.out.print("HAKUNA");
    }
}
 
class Child extends Parent {
    Child() throws Throwable {
        System.out.println("MATATA");
    }
}
 
public class Test {
    public static void main(String[] args) throws Throwable {
        new Child();
    }
}