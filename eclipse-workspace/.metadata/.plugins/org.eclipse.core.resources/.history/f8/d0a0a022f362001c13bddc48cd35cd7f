package test2;

public class Array11 {

	public static void main(String[] args) {

      if (args[0].equals("open"))
         if (args[1].equals("someone"))
            System.out.println("Hello!"); 
      else System.out.println("Go away "+ args[1]); // else always goes nearest if
      else System.out.println("oposos"); //the else goes with the previous unmatched if
    }
}

