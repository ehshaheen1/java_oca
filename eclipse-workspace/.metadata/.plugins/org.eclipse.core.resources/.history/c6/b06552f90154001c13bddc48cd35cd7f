package oca.chapter1;

public class IdentifierAllowJava {
	//There are only three rules to remember for legal identifi ers:
	//	The name must begin with a letter or the symbol $ or _.
	//	 Subsequent characters may also be numbers.
	//	You cannot use the same name as a Java reserved word.
	
	//We cant use special sign *@
	// below variable/identifier are allowed
	int okidentifier;
	int $OK2Identifier;
	int _alsoOK1d3ntifi3r;
	int __SStillOkbutKnotsonice$;
	
	// below variable/identifier are not allowed
	
	public static void main(String[] args) {
		

	}

}
